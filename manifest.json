{
    "name": "QuickConvert",
    "description": "Currency converter",
    "version": "0.1",
    "manifest_version": 3,
    "action": {"default_popup":"index.html"},
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "icons": 
    {
        "128": "./res/ico/128.2.png"
    }
}
